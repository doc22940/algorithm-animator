    "@vue/cli-plugin-typescript": "^3.6.0",
    "@vue/cli-service": "^3.6.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "jest": "^24.8.0",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "ts-jest": "^24.0.2",
    "typescript": "^3.4.3",
    "vue-jest": "^3.0.4",
